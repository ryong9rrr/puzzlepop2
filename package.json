{
  "name": "puzzlepop2",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/**/*"
  ],
  "scripts": {
    "postinstall": "husky install",
    "prettier": "prettier --cache --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "lint": "turbo run lint",
    "format": "yarn prettier && yarn lint",
    "test": "turbo run test",
    "graph": "turbo run build --graph",
    "clean": "turbo run clean && rm -rf .turbo && rm -rf node_modules && rm -rf yarn.lock",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0"
  },
  "resolutions": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "typescript": "5.0.4",
    "@types/node": "^22.10.7"
  }
}
